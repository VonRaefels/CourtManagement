<header role="banner" class="navbar navbar-default navbar-fixed-top">
    <div class="container">
        <div class="navbar-header">
            <button data-target=".bs-navbar-collapse" data-toggle="collapse" type="button" class="navbar-toggle">
            <span class="sr-only">Toggle navigation</span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
            </button>
            <a class="navbar-brand" href="/">
                <%= user.urba.name %>
            </a>
        </div>
        <nav role="navigation" class="collapse navbar-collapse bs-navbar-collapse">
            <ul class="nav navbar-nav">
                <% cuadros.forEach(function(cuadro, index) { %>
                    <li
                        <% if(cuadro._id == active._id) { %>
                            class="active"
                        <% } %>
                    >
                        <a href="/cuadros/<%= cuadro._id %>"><%= cuadro.name %></a>
                    </li>
                <% }); %>
            </ul>
            <ul class="nav navbar-nav navbar-right">
                <li>
                    <button type="button" class="navbar-btn btn btn-primary" id="logout-btn">
                        <span class="glyphicon glyphicon-log-out"></span>&nbsp&nbsp&nbspSalir
                    </button>
                </li>
                <li>
                    <a href="#" id="info"><span class="glyphicon glyphicon-info-sign"></span></a>
                </li>
            </ul>
        </nav>
    </div>
</header>

<div id="info-content" style="display: none">
    <ul id="info-content-list">
        <li>
            <span class="bull bull-libre">&bull;</span>
            <span>Pista libre</span>
        </li>
        <li>
            <span class="bull bull-ocupada">&bull;</span>
            <span>Pista ocupada</span>
        </li>
        <li>
            <span class="bull bull-mine">&bull;</span>
            <span>Tus reservas</span>
        </li>
    </ul>
</div>