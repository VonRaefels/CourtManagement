<header role="banner" class="navbar navbar-default navbar-fixed-top">
    <div class="container">
        <div class="navbar-header">
            <button data-target=".bs-navbar-collapse" data-toggle="collapse" type="button" class="navbar-toggle">
            <span class="sr-only">Toggle navigation</span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
            </button>
            <a class="navbar-brand" href="/"><%= user.urba.name %></a>
        </div>
        <nav role="navigation" class="collapse navbar-collapse bs-navbar-collapse">
            <ul class="nav navbar-nav">
                <% cuadros.forEach(function(cuadro, index) { %>
                    <li
                        <% if(cuadro._id == active._id) { %>
                            class="active"
                        <% } %>
                    >
                        <a href="/cuadros/<%= cuadro._id %>"><%= cuadro.name %></a>
                    </li>
                <% }); %>
            </ul>
            <button type="button" class="navbar-right navbar-btn btn btn-primary" id="logout-btn">
                <span class="glyphicon glyphicon-log-out"></span>&nbsp&nbsp&nbspSalir
            </button>
        </nav>
    </div>
</header>